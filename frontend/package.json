    "@mui/material": "^7.1.1",
    "@shopify/shopify-api": "^7.7.0",
    "@tanstack/react-query": "^5.74.11",
    "@types/js-cookie": "^3.0.6",
    "@types/react-datepicker": "^6.2.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "js-cookie": "^3.0.5",
